{
  "schemaVersion": 1,
  "id": "nopackcompatcheck",
  "version": "${version}",
  "name": "NoPackCompatCheck",
  "description": "Disables the “incompatible” warning when loading resource packs with different mcmeta version.",
  "authors": [
    "Wudji"
  ],
  "contact": {
    "homepage": "https://modrinth.com/project/nopackcompatcheck",
    "sources": ""
  },
  "license": "LGPL-3.0",
  "icon": "assets/nopackcompatcheck/icon.png",
  "environment": "client",
  "entrypoints": {
    "fabric-datagen": [
      "com.wudji.nopackcompatcheck.client.NopackcompatcheckDataGenerator"
    ],
    "client": [
      "com.wudji.nopackcompatcheck.client.NopackcompatcheckClient"
    ],
    "main": [
      "com.wudji.nopackcompatcheck.Nopackcompatcheck"
    ]
  },
  "mixins": [
    {
      "config": "nopackcompatcheck.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "minecraft": "*"
  }
}
